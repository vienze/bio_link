{
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/"
    },
    {
      "source": "/assets/img/:path*",
      "destination": "/"
    },
    {
      "source": "/assets/css/:path*",
      "destination": "/"
    },
    {
      "source": "/assets/js/:path*",
      "destination": "/"
    },
    {
      "source": "/assets/font/:path*",
      "destination": "/"
    }
  ]
}
